                <div class="widget-content clearfix">
                    <div class="wdg-classic-posts color-theme clearfix">
                        <ul class="list-unstyled">
                            <li class="post-item">@Html.ActionLink("எழுத்தாளர்கள்", "BooksSuggestion", "Home", null, new { @class = "bookspace", target = "_blank" })<br /><a href="#" target="_blank">பத்திரிக்கைகள்</a><br /><br /></li>

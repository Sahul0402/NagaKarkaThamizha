                <div class="widget-title clearfix"><h3>@DateTime.Now.AddYears(-1).Year விருதுகள்</h3><div class="sep-widget"></div></div>
                <div class="widget-content clearfix">
                    <div class="wdg-classic-posts color-theme clearfix">
                        <ul class="list-unstyled">
                            <li class="post-item">
                                @Html.ActionLink(@DateTime.Now.AddYears(-1).Year + " சிறந்த நாவல்", "Novels", "Books", new { header = "Novel" }, new { @class = "bookspace", target = "_blank" })<br />

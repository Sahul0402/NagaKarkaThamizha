                                        <div class="form-group">
                                            <input type="text" class="form-control" placeholder="Search" required="required" />
                                            <button type="submit" class="btn-search"><i class="icon-search"></i></button>
                                        </div>
                                    </form>
                                </li>
                            </ul>
                        </li>
